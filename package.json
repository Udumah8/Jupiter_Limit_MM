{
  "name": "solana-market-maker",
  "version": "2.0.0",
  "description": "CLI Solana meme coin market maker bot with Jupiter V6, RFQ, and advanced safety modules",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "test": "node test-imports.js",
    "test:watch": "jest --watch",
    "rebuild": "npm rebuild",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "solana",
    "market-maker",
    "meme-coin",
    "trading-bot",
    "jupiter-v6",
    "rfq",
    "defi",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "directories": {
    "doc": "docs"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@jup-ag/limit-order-sdk": "^0.1.0",
    "@pythnetwork/price-service-client": "^1.5.0",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.91.6",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "dotenv": "^17.2.3",
    "ed25519-hd-key": "^1.3.0",
    "joi": "^17.9.2",
    "node-fetch": "^3.3.2",
    "p-retry": "^6.2.0",
    "winston": "^3.13.0",
    "decimal.js": "^10.4.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@types/bn.js": "^5.1.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@types/node-fetch": "^2.6.13",
    "@types/sqlite3": "^3.1.11",
    "babel-jest": "^30.2.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  }
}
